        </div>
        <div class="ibox-content">
          <form method="get" class="form-horizontal">

            <div class="form-group">
              <label class="col-lg-2 control-label">生日:</label>
                <div class="col-lg-10"><p class="form-control-static"><%= @about_me.birthday %></p></div>
              </div>
            <div class="hr-line-dashed"></div>

            <div class="form-group">
              <label class="col-lg-2 control-label">毕业于:</label>
                <div class="col-lg-10"><p class="form-control-static"><%= @about_me.graduated_from %></p></div>
              </div>
            <div class="hr-line-dashed"></div>

            <div class="form-group">
              <label class="col-lg-2 control-label">网站:</label>
                <div class="col-lg-10"><p class="form-control-static"><%= @about_me.website %></p></div>
              </div>
            <div class="hr-line-dashed"></div>

            <div class="form-group">
              <label class="col-lg-2 control-label">爱好:</label>
                <div class="col-lg-10"><p class="form-control-static"><%= @about_me.hobby %></p></div>
              </div>
            <div class="hr-line-dashed"></div>

            <div class="form-group">
              <label class="col-lg-2 control-label">地址:</label>
                <div class="col-lg-10"><p class="form-control-static"><%= @about_me.address %></p></div>
              </div>
            <div class="hr-line-dashed"></div>

            <div class="form-group">
              <label class="col-lg-2 control-label">标题:</label>
                <div class="col-lg-10"><p class="form-control-static"><%= @about_me.title %></p></div>
              </div>
            <div class="hr-line-dashed"></div>

            <div class="form-group">
              <label class="col-lg-2 control-label">描述:</label>
                <div class="col-lg-10"><p class="form-control-static"><%= @about_me.description %></p></div>
              </div>
            <div class="hr-line-dashed"></div>

            <div class="form-group">
              <div class="col-sm-4 col-sm-offset-0">
                <%= link_to raw("<i class='fa fa-arrow-left'></i> 返回"), about_mes_path,  class: "btn btn-default btn-sm"%>
              </div>
            </div>
          </form>
        </div>
      </div>
